global var x:
_L: z != 1

global var z:
_L: TRUE

local var t:

global var secret:
_L: FALSE

_Gamma_0: z -> LOW, x -> LOW, t -> LOW, secret -> HIGH
_Rely: (Gamma_x => Gamma_x') && (z == 1 => z == z')
_Guar: TRUE

t = 0;
t = CAS(z, 0, 1);

while (t != 0) {
	while (z != 0) {}
	t = CAS(z, 0, 1);
}

x = secret;
