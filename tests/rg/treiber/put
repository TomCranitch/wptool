global obj n:
_Field: val
    _L: n.level == 1
_Field: level
    _L: TRUE
_Field: next
	_L: TRUE

global var *head:
_L: TRUE
_PT: n

global var *ss:
_L: TRUE
_PT: head, n

global var v:
_L: l == 1

global var l:
_L: TRUE

_Rely: TRUE
_Guar: TRUE // (n.level == 0) => (n'.level == 0)

n.level = l;
n.val = v;

do 
_invariant: TRUE
{
	ss = head;
	n.next = ss;
}
while(CAS(head, ss, &n) == 0)

